    + Schema

            {
              "$schema": "http://json-schema.org/draft-04/schema#",
              "type": "array",
              "description": "List of metadata properties",
              "items": {
                "type": "object",
                "properties": {
                  "operation": {
                    "type": "string",
                    "description": "Name of the operation"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "online",
                      "offline"
                    ],
                    "description": "Type of the backup"
                  },
                  "username": {
                    "type": "string",
                    "description": "Name of the triggering user"
                  },
                  "instance_guid": {
                    "type": "string",
                    "description": "GUID of the service instance"
                  },
                  "service_id": {
                    "type": "string",
                    "description": "Service ID of the service instance"
                  },
                  "plan_id": {
                    "type": "string",
                    "description": "Plan ID of the service instance"
                  },
                  "organization_guid": {
                    "type": "string",
                    "description": "Organization GUID of the service instance"
                  },
                  "space_guid": {
                    "type": "string",
                    "description": "Space GUID of the service instance"
                  },
                  "backup_guid": {
                    "type": "string",
                    "description": "GUID of the backup"
                  },
                  "trigger": {
                    "type": "string",
                    "enum": [
                      "manual",
                      "automatic"
                    ],
                    "description": "The way how the backup has been triggered."
                  },
                  "state": {
                    "type": "string",
                    "enum": [
                      "new",
                      "processing",
                      "aborting",
                      "succeeded",
                      "failed",
                      "aborted"
                    ],
                    "description": "The state of the last operation."
                  },
                  "started_at": {
                    "type": "string",
                    "description": "The timestamp of the last operation (UTC format)."
                  },
                  "finished_at": {
                    "type": "string",
                    "description": "The timestamp of the last operation (UTC format)."
                  }
                }
              }
            }
